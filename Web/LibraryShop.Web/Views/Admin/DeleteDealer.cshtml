@model LibraryShop.Web.ViewModels.Administration.Models.DeleteDealerModel

@{
    ViewBag.Title = "Delete Dealer";
}

<h2>@ViewBag.Title</h2>

<div class="row form-group">
    <div class="offset-md-3 col-md-6">
        <form method="post">

            <div class="form-group">
                <label asp-for="Id">Dealer Id:</label>
                <input asp-for="Id">
                <span asp-validation-for="Id" class="text-danger"></span>
            </div>
            <input class="btn btn-primary" type="submit" value="Save" />
        </form>
    </div>
</div>